{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:win": "rd /s /q .next 2>nul & rd /s /q node_modules\\.cache 2>nul",
    "dev:clean": "npm run clean:win && npm run dev",
    "build:clean": "npm run clean:win && npm run build",
    "build:optimized": "powershell -ExecutionPolicy Bypass -File ./build-optimized.ps1",
    "analyze": "npm run build:optimized -- --analyze",
    "type-check": "tsc --noEmit",
    "perf:audit": "npm run build && npm run start & sleep 5 && lighthouse http://localhost:3000 --only-categories=performance --chrome-flags=\"--headless\" --output=json --output-path=./performance-audit.json"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.8",
    "@google-analytics/data": "^5.2.0",
    "@types/dompurify": "^3.0.5",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "20.6.2",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@types/react-window": "^1.8.8",
    "assert": "^2.1.0",
    "autoprefixer": "10.4.15",
    "browserify-zlib": "^0.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "critters": "^0.0.23",
    "crypto-browserify": "^3.12.1",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.1",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.1",
    "firebase": "^11.3.1",
    "firebase-admin": "^13.1.0",
    "google-auth-library": "^10.2.1",
    "googleapis": "^154.1.0",
    "highlight.js": "^11.11.1",
    "https-browserify": "^1.0.0",
    "js-cookie": "^3.0.5",
    "marked": "^16.1.2",
    "next": "^15.1.7",
    "next-sitemap": "^4.2.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.5.2",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "url": "^0.11.4",
    "zod": "^3.22.2"
  },
  "overrides": {
    "undici": "5.25.4"
  }
}
